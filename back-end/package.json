    "cookie-parser": "1.4.4",
    "cors": "2.8.5",
    "debug": "2.6.9",
    "express": "4.16.1",
    "express-validator": "6.1.0",
    "fs": "0.0.1-security",
    "jsonwebtoken": "8.5.1",
    "morgan": "1.9.1",
    "multer": "1.4.2",
    "mysql2": "1.6.5",
    "rimraf": "2.6.3",
    "sequelize": "5.8.12",
    "sequelize-mysql": "1.7.0",
    "socket.io": "2.5.0"
  },
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.5",
